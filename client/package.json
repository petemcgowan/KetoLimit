{
  "name": "KetoLimit",
  "version": "0.0.1",
  "private": true,
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": "universe/native",
    "parserOptions": {
      "es6": true
    },
    "env": {
      "es6": true
    }
  },
  "scripts": {
    "start": "react-native start",
    "ios": "npx react-native run-ios --configuration KetoLimitDebug",
    "ios-ReleaseiPhone15": "npx react-native run-ios --configuration KetoLimitRelease --simulator='iPhone 15'",
    "ios-ReleaseiPhone15Pro": "npx react-native run-ios --configuration KetoLimitRelease --simulator='iPhone 15 Pro'",
    "android": "npx react-native run-android",
    "android-logs": "adb -s b8e614c1f5e5 logcat *:V | grep \"com.buachaillmaith\"",
    "androidRelease": "react-native run-android --variant=release",
    "androidRelease2": "react-native run-android --variant=release2",
    "react-devtools": "react-devtools",
    "start-reset-cache": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint ."
  },
